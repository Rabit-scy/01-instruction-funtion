int foo(int a, int b, int c, int d, int e, int f, int g, int h, int i, int j,
        int k, int l, int m) {

  int sum;

  sum = (a + b + c + d + e + f - g - h - i) * j * k;
  sum = sum * m / l;

  return sum;
}

int main(void) {
  int e, g, l, result;
  e = 3, g = 21, l = 5;
  result = foo(10, 9, 8, 15, e, 20, g, 0, -13, 41, 29, l, 51);

  return result;
}